<% var cdnCombineStr = "" %>

<% if (theme.cdn.mdui.jsdCombine.js) {
  if (!cdnCombineStr) {
    cdnCombineStr = "https://cdn.jsdelivr.net/combine/" + theme.cdn.mdui.jsdCombine.js
  } else {
    cdnCombineStr = cdnCombineStr + "," + theme.cdn.mdui.jsdCombine.js
  }
} else { %>
  <script src="<%- theme.cdn.mdui.js %>"></script>
<% } %>

